{
  "manifest_version": 2,
  "name": "Groovy Light Theme",
  "version": "1.0",

  "theme": {
    "colors": {
      // Color of the window frame
      "frame": "#f2e5bc", // light0_soft
      // Background color of the new tab page
      "ntp_background": "#f2e5bc", // light0_soft
      // Color of the separator line at the top of the toolbar
      "toolbar_top_separator": "#f2e5bc", // light0_soft

      // Background color of the toolbar and tabs
      "toolbar": "#fbf1c7", // light0

      // Background color of text fields in the toolbar
      "toolbar_field": "#ebdbb2", // light1

      // Color of the border around text fields in the toolbar in focus
      "toolbar_field_border_focus": "#bdae93", // bg3

      // Background color of highlighted fields in the toolbar
      "toolbar_field_highlight": "#bdae93", // bg3

      // Text color of highlighted fields in the toolbar
      "toolbar_field_highlight_text": "#3c3836", // fg

      // Background color of highlighted items in a popup window
      "popup_highlight": "#bdae93", // bg3

      // Text color of highlighted items in a popup window
      "popup_highlight_text": "#3c3836", // fg

      // Text color of the currently selected tab
      "tab_background_text": "#3c3836", // fg
      "sidebar": "#f2e5bc", // light0_soft
      "sidebar_text": "#3c3836", // fg
      // "sidebar_border": "#928374",
      "sidebar_highlight": "#ebdbb2", // light1
      "popup": "#f2e5bc", // light0_soft
      "popup_text": "#3c3836", // fg
      "toolbar_field_text": "#3c3836", // fg
      "icons": "#3c3836" // fg

